@model Tuple<List<string>,Guid>

    <table class="table table-hover" style="margin-top:50px;">
        <thead>

            <tr>
                <th>
                    <font style="vertical-align: inherit;">
                        <font style="vertical-align: inherit;">
                            UserName
                        </font>
                    </font>
                </th>
                
            </tr>
        </thead>
        <tbody class="reactable-data">
            @foreach (var item in Model.Item1)
            {

                <tr>
                    <td>
                        <font style="vertical-align: inherit;">
                            <font style="vertical-align: inherit;">
                                @item
                            </font>
                        </font>
                    </td>
                </tr>
            }
        </tbody>
    </table>

<div>
    <a asp-action="Details" asp-route-Id="@Model.Item2"> <i class="glyphicon glyphicon-arrow-left"></i> &nbsp; Back to contest</a>
</div>
